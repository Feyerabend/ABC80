100 EXTEND : REM ABC802 version adopted from ZX81
110 ; CHR$(12) : FOR R%=0% TO 23% : ; CUR(R%,0%) GWHT; : NEXT R%
120 FOR I%=0% TO 63%
130   FOR J%=43% TO 0% STEP -1%
140     LET X=(I%-52)/31
150     LET Y=(J%-22)/31
160     LET XA=0
170     LET YA=0
180     LET ITER%=0%
190     LET XTEMP=XA*XA-YA*YA+X
200     LET YA=2*XA*YA+Y
210     LET XA=XTEMP
220     LET ITER%=ITER%+1
230     IF XA*XA+YA*YA<=4% AND ITER%<200% THEN GOTO 190
240     IF ITER%=200% THEN TXPOINT I%, J%
250   NEXT J%
260 NEXT I%
